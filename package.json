{
  "name": "buslines-blumenau",
  "version": "0.0.1",
  "description": "A simple crawler to capture all bus lines of Blumenau - Santa Catarina",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec babel-node src/index.js",
    "build": "babel src/ -d dist --presets es2015",
    "babel-dev": "babel-node src/index.js",
    "babel-db": "babel-node src/load-db.js",
    "start": "node dist/index.js"
